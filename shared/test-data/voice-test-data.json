    "edge_cases": {
      "description": "Edge cases and error scenarios",
      "tests": [
        {
          "id": "empty_string",
          "input": "",
          "expected_amount": null,
          "expected_currency": null,
          "description": "Empty input string"
        },
        {
          "id": "whitespace_only",
          "input": "   ",
          "expected_amount": null,
          "expected_currency": null,
          "description": "Whitespace only"
        },
        {
          "id": "no_amount",
          "input": "I spent some money on groceries",
          "expected_amount": null,
          "expected_currency": null,
          "description": "No amount detected"
        },
        {
          "id": "no_currency_use_default",
          "input": "I spent 75 on groceries",
          "expected_amount": 75.0,
          "expected_currency": "use_default",
          "description": "No currency specified, should use default"
        },
        {
          "id": "case_insensitive_upper",
          "input": "I spent 50 DOLLARS on groceries",
          "expected_amount": 50.0,
          "expected_currency": "USD",
          "description": "Case insensitive detection (uppercase)"
        },
        {
          "id": "case_insensitive_mixed",
          "input": "I spent 50 DoLLaRs on groceries",
          "expected_amount": 50.0,
          "expected_currency": "USD",
          "description": "Case insensitive detection (mixed case)"
        },
        {
          "id": "multiple_currencies_first_wins",
          "input": "I spent 50 dollars and 30 euros",
          "expected_amount": 50.0,
          "expected_currency": "USD",
          "description": "Multiple currencies mentioned, first one wins"
        },
        {
          "id": "explicit_over_default",
          "input": "I spent 50 euros on dinner",
          "expected_amount": 50.0,
          "expected_currency": "EUR",
          "default_currency": "USD",
          "description": "Explicit currency should override default"
        }
      ]
    },
