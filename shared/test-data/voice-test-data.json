    "amount_extraction": {
      "description": "Test cases for extracting amounts from voice input",
      "tests": [
        {
          "id": "decimal_amount",
          "input": "I paid 99.99 dollars at Amazon",
          "expected_amount": 99.99,
          "expected_currency": "USD",
          "description": "Extract decimal amount"
        },
        {
          "id": "small_decimal",
          "input": "that's 5.50 dollars",
          "expected_amount": 5.50,
          "expected_currency": "USD",
          "description": "Extract small decimal amount"
        },
        {
          "id": "large_amount",
          "input": "I paid 1234.56 euros for the laptop",
          "expected_amount": 1234.56,
          "expected_currency": "EUR",
          "description": "Extract large decimal amount"
        },
        {
          "id": "symbol_before_amount",
          "input": "I spent $50 on coffee",
          "expected_amount": 50.0,
          "expected_currency": "USD",
          "description": "Extract amount when currency symbol is before"
        },
        {
          "id": "symbol_no_space",
          "input": "I just spent ₹20",
          "expected_amount": 20.0,
          "expected_currency": "INR",
          "description": "Extract amount with symbol and no space"
        },
        {
          "id": "rs_abbreviation",
          "input": "I spent Rs 500 on groceries",
          "expected_amount": 500.0,
          "expected_currency": "INR",
          "description": "Extract amount with Rs abbreviation"
        },
        {
          "id": "trailing_zeros",
          "input": "spent 50.00 dollars",
          "expected_amount": 50.0,
          "expected_currency": "USD",
          "description": "Handle trailing zeros in amount"
        },
        {
          "id": "leading_zeros",
          "input": "spent 050 dollars",
          "expected_amount": 50.0,
          "expected_currency": "USD",
          "description": "Handle leading zeros in amount"
        },
        {
          "id": "zero_amount",
          "input": "I spent 0 dollars",
          "expected_amount": 0.0,
          "expected_currency": "USD",
          "description": "Handle zero amount"
        }
      ]
